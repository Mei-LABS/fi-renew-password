{
  "name": "mei-labs/filament-renew-password",
  "description": "Package for manage recurring renew password",
  "keywords": [
    "laravel",
    "filament",
    "renew",
    "password"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mei LABS",
      "email": "codewithmei@project-mei.com"
    }
  ],

  "require": {
    "php": "^8.1",
    "spatie/laravel-package-tools": "^1.16",
    "illuminate/contracts": "^10.0 || ^11.0",
    "illuminate/validation": "^10.0 || ^11.0",
    "illuminate/support": "^10.0 || ^11.0",
    "filament/filament": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "MeiLABS\\Filament\\RenewPassword\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MeiLABS\\Filament\\RenewPassword\\FilamentRenewPasswordServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
